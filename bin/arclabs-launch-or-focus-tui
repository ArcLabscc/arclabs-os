#!/bin/bash

# Launch or focus on a given TUI identified by the passed in as the command.
# Use by commands like arclabs-launch-wifi to ensure there is only one wifi configuration screen open.

APP_ID="org.arclabs.$(basename "$1")"
LAUNCH_COMMAND="arclabs-launch-tui $@"

exec arclabs-launch-or-focus "$APP_ID" "$LAUNCH_COMMAND"
