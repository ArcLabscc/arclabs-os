#!/bin/bash

# Overwrite all the user configs in ~/.config/hypr with the ArcLabs defaults.

arclabs-refresh-config hypr/autostart.conf
arclabs-refresh-config hypr/bindings.conf
arclabs-refresh-config hypr/input.conf
arclabs-refresh-config hypr/looknfeel.conf
arclabs-refresh-config hypr/hyprland.conf
